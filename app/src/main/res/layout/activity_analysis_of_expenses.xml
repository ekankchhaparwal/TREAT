<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Analysis_of_Expenses">

    <LinearLayout
        android:layout_width="312dp"
        android:layout_height="32dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.22000003">

        <TextView
            android:id="@+id/pie3"
            android:layout_width="15dp"
            android:layout_height="37dp"
            android:layout_weight="1"
            android:background="#EF5350" />

        <TextView
            android:id="@+id/piec"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:layout_gravity="center|center_horizontal"
            android:layout_weight="1"
            android:textColor="#100D0D"
            android:textSize="16sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/e2"
        android:layout_width="258dp"
        android:layout_height="44dp"
        android:background="@drawable/top_and_bottom"
        android:gravity="center"
        android:textColor="#191818"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t1"
        app:layout_constraintVertical_bias="0.245" />

    <TextView
        android:id="@+id/e1"
        android:layout_width="259dp"
        android:layout_height="43dp"
        android:background="@drawable/top_and_bottom"
        android:gravity="center"
        android:textColor="#131313"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.203"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t1"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:id="@+id/analysis_text"
        android:layout_width="203dp"
        android:layout_height="44dp"

        android:gravity="center"
        android:text="@string/analysis"
        android:textAlignment="center"
        android:textColor="#05588C"
        android:textSize="28sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/t1"
        android:layout_width="254dp"
        android:layout_height="44dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/top_and_bottom"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/analysis_text" />

    <TextView
        android:id="@+id/t2"
        android:layout_width="254dp"
        android:layout_height="45dp"
        android:background="@drawable/top_and_bottom"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.211"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/t1"
        app:layout_constraintVertical_bias="0.027" /><![CDATA[

    />




    ]]>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="224dp"
        android:layout_height="219dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/e2"
        app:layout_constraintVertical_bias="0.06999999">

        <org.eazegraph.lib.charts.PieChart
            android:id="@+id/piechart"
            android:layout_width="217dp"
            android:layout_height="217dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:padding="6dp">

        </org.eazegraph.lib.charts.PieChart>

    </androidx.cardview.widget.CardView>

    <LinearLayout
        android:layout_width="312dp"
        android:layout_height="32dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.43">

        <TextView
            android:id="@+id/pie1"
            android:layout_width="15dp"
            android:layout_height="37dp"
            android:layout_weight="1"
            android:background="#FFA726" />

        <TextView
            android:id="@+id/piea"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:layout_gravity="center|center_horizontal"
            android:layout_weight="1"
            android:textColor="#100D0D"
            android:textSize="16sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="312dp"
        android:layout_height="32dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintVertical_bias="0.65">

        <TextView
            android:id="@+id/pie2"
            android:layout_width="15dp"
            android:layout_height="37dp"
            android:layout_weight="1"
            android:background="#66BB6A" />

        <TextView
            android:id="@+id/pieb"
            android:layout_width="260dp"
            android:layout_height="match_parent"
            android:layout_gravity="center|center_horizontal"
            android:layout_weight="1"
            android:textColor="#100D0D"
            android:textSize="16sp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>